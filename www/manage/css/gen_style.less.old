/* CSS Document */

.clear {
	clear:both;
}
html, body {
	margin: 0;
	padding: 0;
	font-family:Arial, Helvetica, sans-serif;
	background:#cfe6eb;
	background-image: url(../images/body_bg.jpg);
	background-repeat:repeat-y;
	background-position: center;
}
.blue_bg {
	background:#a5e0f7;
}
.costco_top {
	width:998px;
	height:82px;
	color:#000000;
	font-size:12px;
	border-bottom:1px solid #808080;
}
.costco_header {
	position:relative;
	width:998px;
	height:162px;
	color:#000000;
	margin:0 auto;
	font-size:12px;
	background:#FFF;
	border-right:1px solid #808080;
	border-left:1px solid #808080;
}
.header {
	position:relative;
	width:998px;
	/* height:82px; */
	color:#000000;
	margin:0 auto;
	font-size:12px;
	background:#FFF;
	border-right:1px solid #808080;
	border-left:1px solid #808080;
}
.header a {
	color:#565656;
	text-decoration:none;
}
.header_logo {
	float:left;
	padding-top:8px;
	padding-left:20px;
	background:#FFF;
}
.header_top_bar {
	float:right;
	width:720px;
	height:30px;
	background:url(../images/top_bar_short.jpg);
}
.header_top_bar_short {
	float:right;
	width:660px;
	height:30px;
	background:url(../images/top_bar_short.jpg);
}
.header_phone1 {
	float:left;
	padding-left:24px;
	padding-top:6px;
}
.header_chat {
	float:left;
	padding-left:24px;
	padding-top:6px;
}
.header_email_us {
	float:left;
	padding-left:24px;
	padding-top:6px;
}
.header_idea {
	float:left;
	padding-left:18px;
	padding-top:6px;
}
.header_support {
	float:left;
	padding-left:24px;
	padding-top:6px;
}
.header_cart {
	float:left;
	padding-left:24px;
	padding-top:6px;
}
.header_start_designing {
	position:absolute;
	left:766px;
	margin:0;
	padding:0;
	z-index: 888;
}
.even {
	background-color: #f6f6f6 !important;
}
.odd {
}

