	}
	a {
		color: @darkblue;
		font-weight: bold;	
	}
	a.btn {
		color: black;
		font-weight: normal;
	}
	a.btn-primary, a.btn-warning, a.btn-success, a.btn-info, a.btn-inverse {
		color: white;
		font-weight: normal;	
	}
}
.ask_page {
