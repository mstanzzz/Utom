	background: rgb(220,233,240); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(220,233,240,1) 0%, rgba(240,246,249,1) 19%, rgba(220,233,240,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(220,233,240,1)), color-stop(19%,rgba(240,246,249,1)), color-stop(100%,rgba(220,233,240,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dce9f0', endColorstr='#dce9f0',GradientType=0 ); /* IE6-9 */
	zoom:1;
}
.graygradient {
	background: #feffff;
	background: -moz-linear-gradient(top,  #feffff 0%, #f2f2f2 4%, #f2f2f2 85%, #d8d8d8 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#feffff), color-stop(4%,#f2f2f2), color-stop(85%,#f2f2f2), color-stop(100%,#d8d8d8));
	background: -webkit-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: -o-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: -ms-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: linear-gradient(to bottom,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff', endColorstr='#d8d8d8',GradientType=0 );
	zoom:1;
}
.lightestbluegradient {
	background: #f0f6f9; /* Old browsers */
	background: -moz-linear-gradient(top,  #f0f6f9 0%, #ffffff 75%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f0f6f9), color-stop(75%,#ffffff)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* IE10+ */
	background: linear-gradient(to bottom,  #f0f6f9 0%,#ffffff 75%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f0f6f9', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}
.lightgraygradient {
	background: #f7f7f7; /* Old browsers */
	background: -moz-linear-gradient(top,  #f7f7f7 0%, #ffffff 75%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f7f7), color-stop(75%,#ffffff)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #f7f7f7 0%,#ffffff 75%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #f7f7f7 0%,#ffffff 75%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #f7f7f7 0%,#ffffff 75%); /* IE10+ */
	background: linear-gradient(to bottom,  #f7f7f7 0%,#ffffff 75%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f7f7f7', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}
.herogradient {
	background: #f5f9fa; /* Old browsers */
	background: -moz-linear-gradient(top,  #f5f9fa 0%, #e0e7ed 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f5f9fa), color-stop(100%,#e0e7ed)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #f5f9fa 0%,#e0e7ed 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #f5f9fa 0%,#e0e7ed 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #f5f9fa 0%,#e0e7ed 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #f5f9fa 0%,#e0e7ed 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f9fa', endColorstr='#e0e7ed',GradientType=0 ); /* IE6-9 */
}
.general_page_gradient {
	background: #feffff; /* Old browsers */
	background: -moz-linear-gradient(top, #feffff 19%, #ddf1f9 99%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(19%,#feffff), color-stop(99%,#ddf1f9)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #feffff 19%,#ddf1f9 99%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #feffff 19%,#ddf1f9 99%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #feffff 19%,#ddf1f9 99%); /* IE10+ */
	background: linear-gradient(to bottom, #feffff 19%,#ddf1f9 99%); /* W3C */	
}
.toproundedcorners {
	-webkit-border-radius: 4px 4px 0px 0px;
    border-radius: 4px 4px 0px 0px;	
}
.bottomroundedcorners {
	-webkit-border-radius: 0px 0px 4px 4px;
    border-radius: 0px 0px 4px 4px;	
}
.rightroundedcorner {
	-webkit-border-radius: 0px 8px 0px 0px;
    border-radius: 0px 8px 0px 0px;	
}
.allroundedcorners {
	-webkit-border-radius: 4px;
    border-radius: 4px;	
}
.innershadow {
	-webkit-box-shadow: inset 0px 0px 8px 1px rgba(0, 0, 0, .4);
	box-shadow: inset 0px 0px 8px 1px rgba(0, 0, 0, .4);
}
.innershadowlight {
	-webkit-box-shadow: inset 0px 2px 8px 1px rgba(0, 0, 0, .1);
	box-shadow: inset 0px 2px 8px 1px rgba(0, 0, 0, .1);
}
.dropshadow {
	-webkit-box-shadow:  0px 4px 8px 1px rgba(0, 0, 0, .2);
	box-shadow:  0px 4px 8px 1px rgba(0, 0, 0, .2);
}
.dropshadowlight {
	-webkit-box-shadow:  0px 2px 4px 1px rgba(0, 0, 0, .1);
	box-shadow:  0px 2px 4px 1px rgba(0, 0, 0, .1);
}
.focusglow {
	-webkit-box-shadow: 0px 0px 8px 1px rgba(112,188,214,.3);	
	box-shadow: 0px 0px 8px 1px rgba(112,188,214,.3);
}
.fltlft {
	float: left;	
}
.fltrt {
	float: right;
}
.center-block, .page_actions .btn.center-block {
	display: block;
	margin: 25px auto;
	min-width: 20%;	
}
.mR15 {
	margin-right: 15px;	
}
.mL15 {
	margin-left: 15px;	
}
.mL45 {
	margin-left: 45px;
}
.mT15 {
	margin-top: 15px;	
}
.center-text {
	text-align: center;
}	
.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
 
.clearfix {
	display: inline-block;
}
 
html[xmlns] .clearfix {
	display: block;
}
 
* html .clearfix {
	height: 1%;
}
.valmsg {
	margin-left: 15px;	
}
.progressbar {
	display: block;
	width: 456px;
	height: 28px;
	background: transparent url(../images/wedesign-progress-sprite.gif) no-repeat 0 0;
	&.step2 {
		background-position: 0 -29px;	
	}
	&.step3 {
		background-position: 0 -58px;	
	}
	a {
		width: 152px;
		height: 28px;
		display: block;
		float: left;
		cursor: pointer;	
	}
}
#fileupload, .company_submit_container, .obscure_walls, .best_time {
	display:none;	
}
#fileupload {
	margin-left: 30px;	
}
.fileinput-button {
	position: relative;
	top: 15px;	
}
.colcontainer {
	.clearfix;
	width: 100%;
	.threecols {
		width: 32%;
		margin-right: 2%;
		padding-left: 0;
		padding-right: 0;
		float: left;
			&:last-child{
				margin-right: 0;
		}
		input[type="text"], input[type="email"], input[type="password"], input[type="file"]{
			padding: 8px;
			margin-left: 0;
			margin-right: 0;
		}
		input[type="text"].prepended, input[type="text"].appended{
			width: 50%;
			float: left;
			padding: 7px;
			border-radius: 0px 4px 4px 0px;
			font-size: 14px;
			line-height: 14px;		
		}
		input[type="text"].appended{
			border-radius: 4px 0px 0px 4px;			
		}
	}
	.twocols {
		width: 49%;
		float: left;
		margin-right: 2%;
		padding-left: 0;
		padding-right: 0;
		&:last-child {
			margin-right: 0;	
		}
		input[type="text"], input[type="email"], input[type="password"], input[type="file"], textarea{
			padding: 8px;
			margin-left: 0;
			margin-right: 0;
		}
		input[type="text"].prepended, input[type="text"].appended{
			width: 50%;
			float: left;
			padding: 7px;
			border-radius: 0px 4px 4px 0px;	
			font-size: 14px;
			line-height: 14px;		
		}
		input[type="text"].appended{
			border-radius: 4px 0px 0px 4px;			
		}
	}
	.twocols.rightside {
		text-align: right;
		input[type='checkbox'], input[type='radio'] {
			float: right;
			margin: 10px;	
		}
	}
	.fourcols {
		width: 24%;
		margin-right: 1%;
		padding-left: 0;
		padding-right: 0;
		float:left;	
			&:last-child{
				margin-right: 0;
		}
	}
	.fivecols {
		width: 19%;
		margin-right: 1%;
		padding-left: 0;
		padding-right: 0;
		float: left;
		&:last-child {
			margin-right: 0;	
		}
	}
	.sixcols {
		width: 15.5%;
		margin-right: 1%;
		padding-left: 0;
		padding-right: 0;
		float: left;
		&:last-child {
			margin-right: 0;	
		}
	}
}
.colcontainer.formcols {
	margin: 10px 0;
	.twocols:first-child {
		width: 24%;
	}
	.twocols:last-child {
		width: 74%;
		input[type="text"].url_fragment {
			width: 30%;
		}
	}
	input {
		max-width: 402px;	
	}
}
.colcontainer.specs_holder {
	margin-top: 3px;
	.threecols:nth-child(3n){
		margin-right: 0;
	}
}
.colcontainer.radiocols, .colcontainer.checkboxcols {
	border-top: 1px solid @lightblue;
	margin-top: 15px;
	.radiotoggle, .checkboxtoggle {
		margin: 10px auto;	
	}
	.threecols {
		text-align: center;	
		border-right: 1px solid @lightblue;
		margin-right: 1%;
		.twocols input {
			margin: 15px auto;
		}
	}
	.threecols:last-child {
		border: 0 none;
	}
	.fourcols {
		border-right: 1px solid @lightblue;	
			&:last-child{
				border-right: 0;
		}
	}
	>.twocols {
		text-align: center;	
		border-right: 1px solid @lightblue;
		margin-right: 1%;
		.twocols input {
			margin: 15px auto;
		}
	}
	.twocols:last-child {
		border-right: 0 none;
		margin-right: 0;
	}
}

.wedesignform, 
[class^="ask_"],
[class*=" ask_"],.lightboxholder {
	.w.error {
		text-align: left;
		color: #660000;	
		.validationMessage {
			float: left;	
		}
		input {
			border: 1px solid #F33;
			outline: #F33 auto 5px;
		}
	}
	.w.valid {
		input: border: 1px solid #c2c2c2;
	}
	.w {
		padding: 5px 5px 20px 5px;
		display: block;	
	}
	hr {
		background-color: @legendbg;	
	}
	input[type='text'], input[type='password'] {
		padding: 8px;
		width: 180px;
			&:focus {
				border: 2px solid #6C6;
				outline: #6C6 auto 5px;
			}
	}
	input[type='checkbox']:focus, input[type='radio']:focus {
		border: 2px solid #6C6;
		outline: #6C6 auto 5px;
	}
	#step2 legend {
		background-image:url(../images/wedesign_step2.png);
	}
	#step3 legend {
		background-image: url(../images/wedesign_step3.png);
	}
	#step2, #step3  {
		margin: 0px;
		div.contents {
			display: none;	
		}
	}
	fieldset {
		padding: 0 17px;
		border: none;
		margin: 0px;
		label, input, textarea {
			margin: 8px 0px;	
		}
		.prepend-input, .append-input {
			float: left;
			padding: 7px;
			margin: 5px 0;
			background-color: @white;
			border-radius: 4px 0px 0px 4px;
			font-size: 14px;
			line-height: 16px;		
			border: 1px solid @lightblue;
			border-right: 0 none;
		}
		.append-input {
			border-radius: 0px 4px 4px 0px;			
			border-right: 1px solid @lightblue;
			border-left: 0 none;
		}
		legend {
			display: block;
			color: @legendtxt;
			text-shadow: 0px 1px 0px #fff;
			padding: 10px 0px 10px 34px;
			font-size: 20px;
			font-weight: bold;
			width: 100%;
			margin-left: -17px;
			background:@legendbg url(../images/wedesign_step1.png) no-repeat 8px 9px;
			cursor: pointer;
		}
		h3 {
			color: @darkblue;
			font-weight: normal;	
		}
		p {
			width: 90%;
			em {
				color: #6e7b81;
			}
		}
	}
	.hanging_needs label{
		text-align: right;
		display: block;	
	}
	.accessories label, .contact_information .fourcols {
		text-align: right;
		input[type=text] {
			display:block;
			margin: 0px 0px 10px;
		}
		select{
			margin: 8px 0px;
			float: left;	
		}
	}
	.finish img {
		position: relative;
		top: 10px;
		border: 1px solid #6e7b81;	
	}
}
/*==================================
	New Styles for Ask an Organizer
====================================*/
/*==================================
	Masthead Hide/Show
====================================*/
input[type='search'], .askinput {
	height: 30px;
	-webkit-appearance: none;
	border-radius: 5px;
	border: 0 none;
	box-shadow: inset 0px 1px 3px 0px rgba(0, 0, 0, .4);
}
.masthead {
	position: relative;
	&.hide_masthead {
		position: relative;
		top: 0px;
		left: auto;
		right: auto;
		display: none;
		z-index: 1000;
		.dropshadow;
	}
}
.ask_masthead {
	width: 998px;
	padding-top: 45px;
	margin: 0 auto;
	height: 95px;
	border-left: 1px solid gray;
	border-right: 1px solid gray;
	background: white;
	position: relative;
	.logo {
		margin-left: 10px;
		margin-right: 10px;
		float: left;
		position: relative;
		img {
			max-width: 135px;
			height: auto;
		}
		.hover {
			display:none;
			position: absolute;
			bottom: -30px;
			left: 0px;
			font-size: 11px;
			font-weight: 200;
			color: @darkblue;
		}
		&:hover {
			.hover {
				display: block;	
			}
		}
	}
	.header_start_designing {
		top: 0px;	
	}
	.header_top_bar {
		float: none;
		font-size: 11px;
		position: absolute;
		top: 0px;
		right: 0px;
		width: 620px;
		.phone_number, .showroom_links {
			padding: 7px;
			float:left;
			img {
				padding-right: 5px;
				float: left;
			}
		}
		.header_cart {
			.header_sub_container_cart {
				right: 230px;
				top: 28px;
				left: auto;
			}
		}
	}
}
.trigger_masthead {
	height: 30px;
	width: 300px;
	position: absolute;
	top: 0px;
	right: 5px;
	a {
		padding: 7px 80px;
		text-align: center;
		font-size: 11px;
		color: @gray;
		border: 1px solid white;	
		.graygradient;
		.bottomroundedcorners;
		.dropshadowlight;
	}
}
.ask_mainnav {
	width: 998px;
	padding: 7px 0px;
	margin: 0 auto;
	border-left: 1px solid @mediumgray;
	border-right: 1px solid @mediumgray;
	background: @darkblue;
	.login {
		margin-top: 4px;
		float: left;
		margin-left: 25px;	
	}
	form {
		margin-top: 3px;	
	}
}
.ask_hero {
	width: 998px;
	font-family: 'Helvetica Neue', Helvetica Neue, 'Helvetica', Helvetica, Arial, sans-serif;
	padding: 7px 0px;
	margin: 0 auto;
	text-shadow: 0px 1px 0px #ffffff;
	border-left: 1px solid @mediumgray;
	border-right: 1px solid @mediumgray;
	border-bottom: 1px solid @mediumgray;
	box-shadow:inset 0px -1px 0px 0px rgba(255,255,255,.7), 0px 1px 3px 0px rgba(0, 0, 0, .2);
	z-index: 500;
	position: relative;	
	overflow: hidden;
	.herogradient;
	.ask_pad {
		padding: 20px;	
	}
	img {
		border: 1px solid @lightestblue;
		.dropshadowlight;	
	}
	p.intro {
		font-size: 20px;
		line-height: 1.5em;
		font-weight: 200;
		color: @darkblue;
		margin-left: 15px;
	}
	h2 {
		color: @darkblue;
		margin: 5px 0px 10px;
		margin-left: 15px;
	}
	p.caption {
		font-size: 11px;
		font-weight: bold;
		color: @darkblue;
		margin-left: 15px;
	}
	.profile_pic {
		float: left;
		margin-right: 20px;
		width: 100px;
		height: 100px;
		margin-bottom: 30px;	
	}
	.profile_nav {
		p {
			font-size: 13px;
			line-height: normal;
			margin: 5px 0px;	
		}
		h2 {
			margin: 5px 0px;	
		}
		.btn-group {
			margin-top: 20px;	
		}
	}
	.page_slug {
		color: white;
		position: absolute;
		bottom: 20px;
		right: 30px;
		z-index: 502;
		text-shadow: 0px 1px 3px #000000;
		margin: 0;
		font-size: 32px;
	}
	.right_header_background {
		position: absolute;
		top: 0;
		right: 0;
		z-index: 501;
		box-shadow: none;
		border: 0;
	}
	#askCarousel {
		border: 1px solid white;
		box-shadow: 0px 1px 4px rgba(0, 0, 0, .2);
		.allroundedcorners;
		img {
			border: 0;
			box-shadow: none;	
		}
	}
