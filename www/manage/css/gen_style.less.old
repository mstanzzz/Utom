			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#802e6c81', endColorstr='#802e6c81',GradientType=0 );
		}
	}
}
.idea_container {
    width: 206px;
    max-height: 800px;
    padding: 10px;
    border-radius: 4px;
    background: #f6fafb;
    background: -moz-linear-gradient(top, #f6fafb 0%, #eaf2f7 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f6fafb), color-stop(100%, #eaf2f7));
    background: -webkit-linear-gradient(top, #f6fafb 0%, #eaf2f7 100%);
    background: -o-linear-gradient(top, #f6fafb 0%, #eaf2f7 100%);
    background: -ms-linear-gradient(top, #f6fafb 0%, #eaf2f7 100%);
    background: linear-gradient(to bottom, #f6fafb 0%, #eaf2f7 100%);
    border: 1px solid #cfdce2;
    box-shadow: inset 0 1px 1px #fff, inset 0px -1px 2px #dbe7ee, 0px 1px 3px rgba(0,0,0,.1);
    margin: 20px 0px 0px 20px;
    float: left;
    p {
        color: #494949;
        font-size: 12px;
        line-height: 17px;
    }
    h3 {
        font-size: 16px;
        color: #2e6c81;
        font-weight: bold;
        letter-spacing: normal;
        line-height: 20px;
        margin-bottom: 6px;
        text-shadow: 0px 1px 0px #ffffff;
    }
    .idea_image_content {
        a {
             display: block;
             margin: 0;
             padding: 0;
             width: 206px;
             overflow: hidden;
        }
        img {
            width: 182px;
            height: auto;
            border: 5px solid #fff;
            box-shadow: 0px 1px 3px rgba(0,0,0,.2);
            margin: 4px 6px;
        }
    }
    .idea_link_content {
        border-bottom: 1px solid #cdd3d6;
        box-shadow: 0px 1px 0px #ffffff;
        margin-bottom: 6px;    
        a{
            font-weight: normal;
            font-style: italic;
            padding: 0 0 6px;
            display: inline-block;
        }
    }
}
.idea_left {
    background-color: #e0e7ed;
	min-height: 600px;
    h2 {
        font-size: 20px;
        margin: 15px;
        font-weight: 200;
        letter-spacing: normal;
        color: #494949;
        text-shadow: 0px 1px 0px #fff;
    }
    ul {
         li a {
             padding: 7px 5px 7px 20px;
             display: block;
             font-weight: normal;
             &:hover {
                 background-color: rgba(46,108,129,.5);
                 box-shadow: inset 0px 1px 3px #848484, 0px 1px 0px #ffffff;
                 color: #fff;
                 text-shadow: 0px -1px 0px rgba(46,108,129,.7);
             }
         }
		 li.active a {
			font-weight: bold; 
		 }
         li.add_new {
		 	margin-top: 10px;
		 	a {
				 cursor: pointer;
				 font-weight: bold;
				 border-top: 1px solid #848484;
				 box-shadow: inset 0px 1px 0px #ffffff;
				 &:hover {
					 background-color: #72a15a;
				 	box-shadow: inset 0px -1px 0px #ffffff;
				 }
			}
         }
     }
}
.click_to_view {
    z-index: 40;
    cursor: pointer;
   .hover_msg {
        width: 100%;
        height: 100%;
        background-color: #2e6c81;
        background-color: rgba(46,108,129,.7);
        text-align: center;
        color: #ffffff;
        font-weight: bold;
        font-size: 20px;
        position: absolute;
        top: -20px; 
		left: 0px;
        z-index: 50;
        border-radius: 4px; 
        span {
            position: absolute;
            top: 25px;
            left: 25px;
        }        
    }        
}
.idea_description {
	margin: 20px 20px 10px 20px;
	h2 {
		font-size: 26px;
		color: @darkblue;
		font-weight: 100;
		strong {
			font-weight: bold;	
		}
	}
	p {
		margin: 10px 0px;
		color: #494949;
		line-height: normal;
	}
}
#idea_lightbox {
	display: none;
	width: 580px;
	font-family: 'Helvetica Neue', Helvetica Neue, 'Helvetica', Helvetica, Arial, sans-serif;
 	.lightboxholder {
		width: 100%;
		position: relative;
		.buttonbar {
			border-top: 1px solid #eeeeee;
			position: absolute;
			bottom: 0;
			left: 0;
			width: 100%;
			height: 50px;
			a.btn, div.btn-group {
				float: left;
			}
			.btn {
				margin: 10px 15px;	
				font-family: Helvetica, Arial, sans-serif;
				letter-spacing: normal;
				font-weight: normal;
			}
		}
	}
	img {
		max-width: 500px;
		max-height: 500px;
		width: auto;
		height: auto;
		margin: 10px auto;
		display: block;	
		border: 5px solid #fff;
		box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
	}
	.edit_image img {
		width: 50px;
		height: auto;	
	}
}

#idea_lightbox .dropdown-menu{
	text-shadow: none;
	background-color: #eeeeee;
	cursor: pointer;
	a {
		color: #545454;	
	}
	li > a:hover, li > a:focus, .dropdown-submenu:hover > a {
		color: #ffffff;
		background-color: #666;
		background-image: none;
		background-image: none;
		background-image: none;
		background-image: none;
		background-image: none;
		background-image: none;
		filter: none;
	}
}




