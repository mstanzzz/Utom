.header_sub_container_cart {
  position: relative;
  left: 1px;
  top: 9px;
  width: 212px;
  z-index: 999;
  background-color: #ececec;
  border-left: 1px solid #c2c2c2;
  border-right: 1px solid #c2c2c2;
  border-bottom: 1px solid #c2c2c2;
  color: #565656;
}



.header_sub_container_support {
	position:absolute;
	top:30px;
	width:190px;
	z-index: 999;
	background-color: #ececec;
	border-left:1px solid #c2c2c2;
	border-right:1px solid #c2c2c2;
	border-bottom: 1px solid #c2c2c2;
	color: #565656;
}
.header_sub_container_support_costco {
	position:absolute;
	top:112px;
	width:190px;
	z-index: 999;
	background-color: #ececec;
	border-left:1px solid #c2c2c2;
	border-right:1px solid #c2c2c2;
	border-bottom: 1px solid #c2c2c2;
	color: #565656;
}
.header_sub {
	display:none; /*--Hidden for those with js turned off--*/
}
.header_sub ul {
	margin: 0;
	padding:0;
	list-style: none;
	padding-top:10px;
}
.header_sub ul li {
	position: relative;
	padding:6px 0 6px 16px;
	border-bottom: 1px solid #c2c2c2;
}
.header_sub li ul {
	position: absolute;
	left:160px;
	top: 0;
	display: none;
	padding-top:0 !important;
}
.header_sub a:hover {
	color:#8bcde7 !important;
}
.page_top_spacer {
	height:16px;
}
.header_cart_row {
	cursor:pointer;
}
.tooltip {
	display:none;
	font-size:12px;
	height:70px;
	width:160px;
	padding:25px;
	color:#fff;
}
.page_title {
	font-size:24px;
	color:#3f6e84;
