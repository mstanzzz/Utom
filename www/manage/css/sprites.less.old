//30px standard content icons
[class^="icon-"],
[class*=" icon-"] {
  display: inline-block;
  width: 30px;
  height: 30px;
  .ie7-restore-right-whitespace();
  line-height: 30px;
  vertical-align: text-top;
  background-image: url("@{iconSpritePath}");
  background-position: 0px 0px;
  background-repeat: no-repeat;
  margin-top: 0px;
}
.btn .icon-triangle-right, .btn .icon-triangle-left {
	width: 15px;
	height: 15px;
	line-height: 15px;	
}
