/*====================================
	Top Nav Bar / Progress Styles
=====================================*/
.top_nav_bar {
	background-color: #eee;
	margin: 0 auto;
	width: 876px;
	padding: 12px;
	a {
		color: #787878;
		font-size: 13px;
		text-decoration: none;
		float:left;
		margin-right: 25px;
		margin-left: 25px;
	}
	#progressbar {
		float: left;
		width: 600px;
		height: 25px;
		border-bottom: 1px solid white;
		border-top: 1px solid #CCC;
		border-left: 1px solid #CCC;
		border-right: 1px solid #CCC;
		box-shadow: inset 0 1px 3px #BBB;
		background-color: #CCC;	
	}
}
.top_nav_box {
	float: left;
	height: 22px;
	text-align: center;
}
.top_nav_bar_over {
	background: none repeat scroll 0 0 #CCCCCC;
}
.progress_list {
	font-size: 18px;
	li {
		padding: 15px;	
		color: #333;
		a {
			color: #333;
		}
		&:before {
			content: "\2610 \a0 \a0 \a0";
		}
	}
	li.completed {
		text-decoration: line-through;
		color: #CCC;
		a {
			color: #CCC
		}
		&:before {
			content: "\2611 \a0 \a0 \a0";
		}
	}
}
/*====================================
	Category Tree Styles
====================================*/
#categorytree {
	padding-bottom: 25px;	
}
.tree { 
	font-size:1.2em;
}
.tree,.tree ul,.tree li { 
	list-style:none; margin:0; padding:0; zoom: 1;
}
.tree ul { 
	margin-left: 20px;
}
.tree li a { 
	color:#555; 
	padding:.1em 7px .1em 27px; 
	display:block; 
	text-decoration:none; 
	border:1px dashed #fff; 
	background:url(../images/icon-file.gif) 5px 50% no-repeat; 
}
.tree li a.tree-parent { 
	background:url(../images/icon-folder-open.gif) 5px 50% no-repeat; 
}
.tree li a.tree-parent-collapsed { 
	background:url(../images/icon-folder.gif) 5px 50% no-repeat; 
}
.tree li a:hover,.tree li a.tree-parent:hover,.tree li a:focus,.tree li a.tree-parent:focus,.tree li a.tree-item-active { 
	color:#000; 
	border:1px solid#eee; 
	background-color:#fafafa; 
	-moz-border-radius:4px; 
	-webkit-border-radius:4px; 
	border-radius:4px; 
}
.tree li a:focus,.tree li a.tree-parent:focus,.tree li a.tree-item-active { 
	border:1px solid #e2f3fb; 
	background-color:#f2fafd; 
}
.tree ul.tree-group-collapsed { 
	display:none; 
}
.categorytreecontainer {
	position: relative;	
}
.tree.selectcattree {
	li {
		border-bottom: 1px solid @blue;
		position: relative;
		.checkbox {
			position: absolute;
			top: 3px;
			right: 5px;	
		}
		img {
			width: 30px;
			height: auto;
			margin-right: 10px;	
		}
	}
}
.categoryoptions {
	background-color: @lightestblue;
	margin-right: 15px;
	padding: 15px;
	width: 300px;
	position: fixed;
	.innershadow;
	.allroundedcorners;
	h3 {
		margin: 0 0 15px;
		color: @darkblue;
		font-size: 15px;	
	}
	h4 {
		display: block;
		margin: 0px 10px 5px;
		padding-bottom: 6px;
		border-bottom: 1px solid @blue;
		font-size: 18px;
		color: #000;
		text-shadow: 0px -1px 0px #fff;
		
	}
	img {
		width: 80px;
		height: auto;
		margin: 0 15px;
		float: left;
		border:2px solid @lightestblue;
		.dropshadow;
	}
	.btn {
		margin: 10px 10px 10px 0px;
	}
}

/*====================================
	Navigation Styles
====================================*/
ul#manage_navigation {
	width: 95%;
	display: block;
	list-style: none inside;
	margin: 0;
	padding: 0;
	color: @navtext;
	> li {
		border: 1px solid @navborder;
		display: block;
		text-indent: 7px;
		margin: 10px 0px;
		padding: 0;
		.navbackground;
		.allroundedcorners;
		a, a:visited {
			display: block;
			padding: 15px 0px;
			font-size: 13px;
			color: @navtext;
			text-indent: 23px;
		}
		a:hover {
			background: rgba(46,108,129,.3);	
		}
		ul {
			display: none;
			list-style: none inside;
			margin: 0;
			padding: 0;
			background-color: @subnavbackground;
			.bottomroundedcorners;
			li {
				display: block;
				list-style: none inside;
				width: 100%;
				background: @subnavbackground none no-repeat;
					.active {
						position: relative;
						background: @activebackground;
						color: #fff;
						/* :-( Mike doesn't like the text shadow
						text-shadow: 0px 1px 0px #000;
						filter: dropshadow(color=#000, offx=0, offy=-1); */
					}
					.active:after {
						left: 100%;
						border: solid transparent;
						content: " ";
						height: 0;
						width: 0;
						position: absolute;
						pointer-events: none;
					}
					
					.active:after {
						border-left-color: @activebackground;
						border-width: 10px;
						top: 50%;
						margin-top: -10px;
					}
				a {
					font-size: 13px;
					/* :-(
					text-shadow: 0px -1px 0px #dee9ef;
					filter: dropshadow(color=#dee9ef, offx=0, offy=-1);
					*/
					text-indent: 14px;
				}
			}
			li.active {
				background-color: @navborder;
			}
			li.otg-specific {
				background-color: @lightorange;	
			}
		}
		ul.active {
			display:block;	
		}
	}
}
