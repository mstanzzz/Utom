}


.link {
	color:#3f6e84;
	font-size:14px;
}
.link a {
	color:#3f6e84;
	font-size:14px;
}
.link a:hover {
	color:#000;
}



.img_box {
	float: left;
	padding-bottom: 16px;
	padding-right: 16px;
	padding-top: 16px;
}

.red_box {
