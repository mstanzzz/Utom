@import "reset.css";
@import "button-icon-styles.css";
body {
	text-align: left;
	margin: 0;
	padding: 0;
	background: url("../images/body_bg.jpg") repeat-y scroll center center #CFE6EB;
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
body.printable_page, body.lightbox {
	background: #fff;	
}
.print_container {
	width: 612pt;
	margin: 0 auto;
	font-size: 12pt;
	padding-bottom: 32pt;
	table {
		td {
			border-bottom: 1pt solid #CCCCCC;	
		}
		.section_heading {
			border-bottom: none;
			border-top:	2pt solid #000;
		}
		label {
			color: #000;
			margin-left: 12pt;
		}
	}
	h1 {
		line-height: 24pt;
		font-size: 24pt;	
	}
}
a {
	color: #154A60;
	text-decoration:none;
}
.clear {
	clear: both;
	width: 0;
}
.msg {
	background: none repeat scroll 0 0 #FFCCCC;
	border-color: #C6C6C6;
	border-style: solid;
	border-width: 1px;
	margin-bottom: 12px;
	padding-bottom: 6px;
	padding-left: 8px;
	padding-top: 6px;
	width: 814px;
}
.form_label {
	color: #5A7F8F;
	font-size: 16px;
}
.manage_form_left_label {
	color: #5A7F8F;
	float: left;
	font-size: 16px;
	padding-top: 6px;
	width: 250px;
}
.manage_form_right_info {
	float: left;
	font-size: 16px;
	padding-top: 6px;
}
.manage_form_block_spacer {
	height: 18px;
}
.manage_main_page_title {
	color: #5A7F8F;
	font-size: 24px;
	padding-bottom: 6px;
	padding-top: 6px;
}
.manage_side_nav_element_head {
	background: none repeat scroll 0 0 #F0F6F9;
	border-color: #DCE9F0;
	border-style: solid;
	border-width: 1px;
	padding-bottom: 8px;
	padding-left: 8px;
	padding-top: 8px;
	margin: 0 0 1px;
	color:#70bcd6;
	font-weight:bold;
	cursor:pointer;
}
.manage_side_nav_element {
	background: none repeat scroll 0 0 #f9fbfc;
	border-left:1px solid #DCE9F0;
	border-right:1px solid #DCE9F0;
}
.manage_side_nav_element a {
	text-decoration:none;
	color:#154a60;
	padding:10px 5px 10px 20px;
	display:block;
	font-size:14px;
}
.manage_side_nav_element a:hover, .manage_side_nav_element .active {
	background:url(../images/left_hover.PNG) #f3f7f9 no-repeat 0 center;
	font-weight:bold;
}
.manage_row_container {
	border-bottom: 1px solid #C6C6C6;
	width: 824px;
}
.manage_head_ctg {
	float: left;
	font-size: 24px;
	padding-left: 10px;
}
.manage_welcome {
	float:right;
}
.manage_welcome ul {
	float:left;
	margin:0;
	padding:0;
}
.manage_welcome ul li {
	float:left;
	list-style-type:none;
}
.manage_welcome ul li a {
	text-decoration:none;
}
.manage_head_logout {
	padding-right: 16px;
	padding-top: 0px;
}
.manage_head_welcome {
	padding-right: 20px;
	padding-top: 5px;
}
.manage_content_management {
	padding-right: 20px;
	padding-top: 5px;
}
.table_head {
	font-size: 14px;
	border-collapse:collapse;
}
.table_head td {
	border: 1px solid #DCE9F0;
	margin:1px 0;
}
.table_head th {
	background:#F0F6F9;
}
.table_head td {
	text-align:center;
}
.table_head td a {
	text-align:center;
}


.top_right_link {
	float:right;
	padding-right:170px;
	padding-top:6px;
	color:#3f6e84;
	font-size:14px;
}
.top_right_link a {
	color:#3f6e84;
	font-size:14px;
}
.top_right_link a:hover {
	color:#000;
}
.top_link {
	float: right;
	padding-right: 20px;
	position: relative;
	top: 20px;
	background:#F0F6F9;
	padding:5px 15px;
	color:#70BCD6;
	margin-right:10px;
	font-weight:bold;
	border:1px solid #DCE9F0;
}


.top_link a {
	color:#70BCD6;
}

.top_link a:hover {
