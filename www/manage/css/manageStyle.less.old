	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#81c6dc), color-stop(100%,#60b2d0)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #81c6dc 0%,#60b2d0 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #81c6dc 0%,#60b2d0 100%); /* Opera 11.10+ */
