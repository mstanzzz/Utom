}
.dropshadow {
	-webkit-box-shadow:  0px 4px 8px 1px rgba(0, 0, 0, .2);
	box-shadow:  0px 4px 8px 1px rgba(0, 0, 0, .2);
}
.dropshadowlight {
	-webkit-box-shadow:  0px 2px 4px 1px rgba(0, 0, 0, .1);
	box-shadow:  0px 2px 4px 1px rgba(0, 0, 0, .1);
}
.focusglow {
