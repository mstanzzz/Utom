/* Form Validation */
.validationMessage {
	color: @orange;
	padding: 4px;
	display: inline-block;	
}
.validationNotification ul {
	list-style: inside disc;	
}
.twocols.error input, .error input, .error input[type="text"] {
	border: 1px solid @orange;
}
input[type="text"],input[type="email"],input[type="password"], input[type="file"], textarea,
{
	padding: 7px;
	margin: 5px;
	background: @lightestblue;
	border: 1px solid @lightblue;
	.allroundedcorners;
	box-shadow: inset 0px 2px 4px 0px rgba(0,0,0,.1);
	-webkit-appearance: none;
	-moz-appearance: none;
	&:focus {
		.focusglow;
		outline-color: @blue;
	}
}
input[name*="order"], .colcontainer .twocols input[name*="order"][type="text"], .colcontainer .threcols  input[name*="order"][type="text"], input[name*="display"]{
	width: 30px;	
}
textarea {
	max-width: 760px;
	width: 90%;
}
.selectwrapper {
	height: 34px;
	max-width: 350px;
	width: 100%;
	overflow: hidden;
	background: @lightestblue url('../images/selectarrow.png') no-repeat right;	
	border: 1px solid @lightblue;
	box-shadow: inset 0px 1px 4px 0px rgba(0,0,0,.1);
	border-radius: 8px;
	select {
		padding: 8px;
		background: none repeat scroll 0 0 transparent;
		border: none;
		font-size: 13px;	
		margin: 0;
		width: 108%;
		-webkit-appearance: none;
		&:focus {
			-webkit-appearance: none;
		}
	}
	&:focus {
		.focusglow;
		outline-color: @blue;
	}
}
/*select {
	width: 92%;	
}*/
.jumpMenu {
	margin-left: 10px;	
}
input[type="radio"], input["type=checkbox"] {
	display: inline-block;
	font-size: 15px;
	line-height: 1em;
	margin: 0 0.25em 0 0;
	padding: 0;
	width: 1.25em;
	height: 1.25em;
	-webkit-border-radius: 0.25em;
	vertical-align: text-top;	
	background: transparent;
	&:focus {
		.focusglow;
		outline-color: @blue;
	}
}
.input_note {
	font-size: 11px;
	font-style: italic;
	color: @gray;
	display: block;
	clear: both;
}
fieldset {
	padding: 7px;
	border: none;
	margin: 10px 0px;
	label, input, textarea {
		margin: 8px 0px;	
	}
	.prepend-input, .append-input {
		float: left;
		padding: 7px;
		margin: 5px 0;
		background-color: @white;
		border-radius: 4px 0px 0px 4px;
		font-size: 14px;
		line-height: 16px;		
		border: 1px solid @lightblue;
		border-right: 0 none;
	}
	.append-input {
		border-radius: 0px 4px 4px 0px;			
		border-right: 1px solid @lightblue;
		border-left: 0 none;
	}
}
fieldset.before_image, fieldset.after_image {
	display: none;	
}
.featured_image .center-block, .featured_image .center-text {
	margin-top: 5px;
	margin-bottom: 5px;	
}
label {
	padding: 5px 0px;
	color: @labeltext;
	font-size: 13px;
	font-style: italic;
	display: block;
	cursor: default;
}
.leftlabel, input[type='checkbox'].leftlabel {
	float: left;
	margin: 15px 15px 25px 0px;
