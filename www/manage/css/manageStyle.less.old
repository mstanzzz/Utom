	-webkit-border-radius: 0px 0px 4px 4px;
    border-radius: 0px 0px 4px 4px;	
}
.rightroundedcorner {
	-webkit-border-radius: 0px 8px 0px 0px;
    border-radius: 0px 8px 0px 0px;	
}
.allroundedcorners {
	-webkit-border-radius: 4px;
    border-radius: 4px;	
}
.innershadow {
	-webkit-box-shadow: inset 0px 0px 8px 1px rgba(0, 0, 0, .4);
	box-shadow: inset 0px 0px 8px 1px rgba(0, 0, 0, .4);
}
