.navbackground {
	background: rgb(220,233,240); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(220,233,240,1) 0%, rgba(240,246,249,1) 19%, rgba(220,233,240,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(220,233,240,1)), color-stop(19%,rgba(240,246,249,1)), color-stop(100%,rgba(220,233,240,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(220,233,240,1) 0%,rgba(240,246,249,1) 19%,rgba(220,233,240,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dce9f0', endColorstr='#dce9f0',GradientType=0 ); /* IE6-9 */
	zoom:1;
}
.graygradient {
	background: #feffff;
	background: -moz-linear-gradient(top,  #feffff 0%, #f2f2f2 4%, #f2f2f2 85%, #d8d8d8 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#feffff), color-stop(4%,#f2f2f2), color-stop(85%,#f2f2f2), color-stop(100%,#d8d8d8));
	background: -webkit-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: -o-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: -ms-linear-gradient(top,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	background: linear-gradient(to bottom,  #feffff 0%,#f2f2f2 4%,#f2f2f2 85%,#d8d8d8 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff', endColorstr='#d8d8d8',GradientType=0 );
	zoom:1;
}
.lightestbluegradient {
	background: #f0f6f9; /* Old browsers */
	background: -moz-linear-gradient(top,  #f0f6f9 0%, #ffffff 75%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f0f6f9), color-stop(75%,#ffffff)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #f0f6f9 0%,#ffffff 75%); /* IE10+ */
	background: linear-gradient(to bottom,  #f0f6f9 0%,#ffffff 75%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f0f6f9', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}
.lightgraygradient {
	background: #f7f7f7; /* Old browsers */
	background: -moz-linear-gradient(top,  #f7f7f7 0%, #ffffff 75%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f7f7), color-stop(75%,#ffffff)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #f7f7f7 0%,#ffffff 75%); /* Chrome10+,Safari5.1+ */
