.radiotoggle, .checkboxtoggle, .data_table .radiotoggle, .data_table .checkboxtoggle {
	width: 65px;
	height: 25px;
	position: relative;
	overflow: hidden;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	-webkit-box-shadow: inset 0px 0px 7px 0px rgba(0, 0, 0, .3);
	box-shadow: inset 0px 0px 7px 0px rgba(0, 0, 0, .3);
	cursor: pointer;
	&.disabled {
		cursor: default;
		opacity: .5;
	}
	.switch {
		width: 25px;
		height: 21px;
		background: #f5f6f6;
		background: -moz-linear-gradient(top, #f5f6f6 0%, #dbdce2 21%, #b8bac6 49%, #dddfe3 80%, #f5f6f6 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f5f6f6), color-stop(21%, #dbdce2), color-stop(49%, #b8bac6), color-stop(80%, #dddfe3), color-stop(100%, #f5f6f6));
		background: -webkit-linear-gradient(top, #f5f6f6 0%, #dbdce2 21%, #b8bac6 49%, #dddfe3 80%, #f5f6f6 100%);
		background: -o-linear-gradient(top, #f5f6f6 0%, #dbdce2 21%, #b8bac6 49%, #dddfe3 80%, #f5f6f6 100%);
		background: -ms-linear-gradient(top, #f5f6f6 0%, #dbdce2 21%, #b8bac6 49%, #dddfe3 80%, #f5f6f6 100%);
		background: linear-gradient(to bottom, #f5f6f6 0%, #dbdce2 21%, #b8bac6 49%, #dddfe3 80%, #f5f6f6 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f6f6', endColorstr='#f5f6f6', GradientType=0 );
		zoom:1;
		-webkit-border-radius: 4px;
		border-radius: 4px;
	}
	.radioinput, .checkboxinput {
		position: absolute;
		bottom: -25px;
		left: 0px;
	}
	.ontext, .offtext {
		display: none;	
		position: absolute;
		top: 5px;
		font-size: 13px;
	}
}
.radiotoggle.on, .checkboxtoggle.on {
	background-color: #2e6c81;
	color: #fff;
	text-shadow: 0px 1px 0px #000;
	filter: dropshadow(color=#000, offx=0, offy=1);
	.ontext {
		display: block;
		left: 5px;
	}
}
.radiotoggle.off, .checkboxtoggle.off {
	background-color: #f0f6f9;
	color: #545c60;
	.offtext {
		display: block;
		right: 5px;
	}
}

