.tabs { 
	height:27px;
	width:645px;
	font-family:Arial, Helvetica, sans-serif;
	margin:40px 0 0 0;
	float:left;

}

.tabs .w1 			{ background-position: -519px 0; width:134px; }
.tabs .w1:hover 	{ background-position: -519px -31px; }
.tabs .w1.current { background-position: -519px -62px; }

/* width 2 */
.tabs .w2 			{ background-position: -366px -0px; width:154px; }
.tabs .w2:hover 	{ background-position: -366px -31px; }
.tabs .w2.current { background-position: -366px -62px; }


/* width 3 */
.tabs .w3 			{ background-position: -193px -0px; width:174px; }
.tabs .w3:hover 	{ background-position: -193px -31px; }
.tabs .w3.current { background-position: -193px -62px; }

/* width 4 */
.tabs .w4 			{ background-position: -0px -0px; width:194px; }
.tabs .w4:hover 	{ background-position: -0px -31px; }
.tabs .w4.current { background-position: -0px -62px; }


/* initially all panes are hidden */ 
.panes .pane {
	display:none;		
}

.panes {
	width:540px;
/*	background:url(../images/tab-content.png) no-repeat;*/
	height:300px;
	margin:0 0 0 25px;
}
.panes div {
	display:none;		
	border-top:0;
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	color:#000;
	line-height:18px;
}
a.current{
	margin:1px 0 0 0;
}

.popup-jobs-page .jobbodybg {
	width:862px;
	height:299px;
	float:left;
	background:url(../images/job-p-bodybg.png) repeat-x;
}
.popup-jobs-page .jobbodybg .jobtree {
	width:287px;
	height:245px;
	float:right;
	z-index:99999;
	background:url(../images/job-tree.png) no-repeat;
	margin:55px 0 0 0;
}
.jobpagebodytext {
	width:160px;
	height:290px;
	float:left;
	margin:0 0 0 22px;
	
}
.jobbody-links {
	width:170px;
	height:290px;
	padding:0 0 0 0;
	float:left;
	border-right:1px dashed #d5d4d4;
}
.jobbody-links ul {
	width:170px;
	height:290px;
	float:left;
	padding:0 0 0 0;
	list-style:inline;
}
.jobbody-links ul li {
	float:left;
	list-style:none;
	padding:0 0 0 0;
}
.jobbody-links ul li a {
	width:170px;
	float:left;
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#000;
	text-decoration:none;
	padding:3px 20px 5px 10px;
}

.popup-jobs-page .jobbottomcurve {
	width:862px;
	height:24px;
	float:left;
	background:url(../images/job-p-bottom-curve.png) no-repeat;
}
.popup-jobs-page .jobjobsearchbtn {
	width:234px;
	height:45px;
	float:right;
	margin:0 80px 0 0;
}
.warning{color:red;background:#ffffd7;padding:5px;margin:5px 0;background:url(../images/info.PNG) no-repeat;height:25px;
}





