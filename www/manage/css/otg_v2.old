	.header-search {
		position: absolute;
		bottom: 5px;
		right: 5px;
		margin-bottom: 0;
		.header-search-input {
			width: 130px;
			position: relative;
			top: -2px;
			padding: 2px 5px;	
		}
		.btn {
			position: relative;
			top: -2px;	
		}
	}
	
	.cta {
		position: absolute;
		top: 0px;
		right: 0px;
		z-index: 500;	
	}
	.contact-info {
		position: absolute;
		top: 60px;
		right: 260px;
		font-size: 24px;
		color: @darkblue;
		text-align: right;
		font-weight: 200;
		a {
			font-weight: 200;	
		}
		.chat {
			margin-left: @gridGutterWidth;	
		}
		.phone i, .chat i {
			position: relative;
			top: 5px;	
		}
	}
}
#menu-button {
	display: none;	
}

.container {
	background-color: @containerBackground;
	box-shadow: 0px 0px 25px rgba(0,0,0,.25);
}
.sidebar-left {
	text-shadow: 0px 1px 0px #fff;
	aside {
		box-shadow: inset 0px 0px 3px 1px rgba(0, 0, 0, 0.15);
		background-color: @lightestgray;
		.border-radius(6px);
	}
}
.sidebar-right {
	aside {
		box-shadow: inset 0px 0px 3px 1px rgba(0, 0, 0, 0.15);
		background-color: @lightestgray;
		.border-radius(6px);
	}
}
.sidebar-left aside, .sidebar-right aside{
	width: (@gridColumnWidth*3)+(@gridGutterWidth*2);
	h5.sidebar-heading, em.sidebar-heading {
		color: @darkestgray;
		background-color: rgba(0,0,0,.1);
		padding-top: 10px;
		margin-top: 0px;
		&.top-radius {
			.border-top-radius(4px);	
		}
	}
	.group_heading {
		font-weight: lighter;
		font-weight: 200;	
	}
	.nested-links h5{
		box-shadow: none;
		border: none;
		padding-left: 0;
	}
	.nested-links a p {
		padding-left: 0;	
	}
	h1,h2,h3,h4,h5,li,p {
		padding-left: @gridGutterWidth;
	}
	h1,h2,h3,h4,h5 {
		border-bottom: 1px solid @white;
		box-shadow: inset 0px -1px 0px 0px @gray;
		padding-bottom: 5px;
		margin-bottom: 0px;
	}
	ul {
		margin-bottom: 0px;	
	}
}
.sidebar-left aside .applied_filters {
	h5 {
		box-shadow: none;
		border: none;
	}
	a {
		font-weight: bold;
		color: @darkestgray;
		i {
			margin-top: 2px;
		}
	}
	.unlist;
	li {
		.unlist;	
	}
	background-color: rgba(0,0,0,.1);
	padding-top: 5px;
	padding-bottom: 5px;
	margin-top: 0px;	
	border-bottom: 1px solid @white;
	box-shadow: inset 0px -1px 0px 0px @gray;
	display: none;
}
.page-content {
	position: relative; //to fix stacking order	
	border-left: @gridGutterWidth solid @white;
	border-right: @gridGutterWidth solid @white;
	padding-bottom: @gridGutterWidth*5;
}
footer, footer.container {
	width: (@gridColumnWidth*12)+(@gridGutterWidth*13);
	color: @darkestgray;
	position: relative;
	text-shadow: 0px 1px 0px @white;
	margin-bottom: @gridGutterWidth;
	a {
		color: @darkestgray;
		font-weight: normal;
	}
	a:hover {
		color: @darkblue;
		text-decoration: underline;	
	}
	#gradient > .vertical(@white, @lightgray);
	.border-bottom-right-radius(8px);
	.border-bottom-left-radius(8px);
	.row {
		margin-left: 0;
		[class^="span"],
		[class*=" span"] {
			padding: @gridGutterWidth/2 0;
		}
	}
	.pull-left {
		margin-right: @gridGutterWidth/2;	
	}
	[class^="social-"] {
		margin-right: 3px;
	}
	h5 {
		text-transform: uppercase;
		color: @darkergray;
		font-size: 13px;
	}
	ul {
		.unlist;
	}
	.footer-top-row {
		box-shadow: inset 0px -3px 5px 0px @white, inset 0px 3px 5px 0px @white;
		background-color: @lightergray;
		.span3 {
			position: relative;
		}
		.span3:after {
			content: '';
			position: absolute;
			top: @gridGutterWidth;
			right: -5px;
			width: 1px;
			height: 110px;
			background-color: @gray;				
		}
		.span3:last-child:after {
			width: 0;	
		}
		.icon-misc-envelope {
			position: relative;
			top: -3px;	
		}
	}
	.footer-middle-row {
		border-top: 1px solid @gray;
		box-shadow: inset 0px 1px 0px 0px @white;
		input {
			width: 180px;	
		}
		h5.pull-left, p.pull-left {
			margin-right: 15px;
		}
		.span4 {
			border-right: 1px solid @gray;
			box-shadow: inset -1px 0px 0px 0px @white;
			margin-left: 19px;	
		}
	}
	.footer-bottom-row {
		border-top: 1px solid @gray;
		box-shadow: inset 0px 1px 0px 0px @white;
		.span2.col5 {
			width: (@gridRowWidth - (@gridGutterWidth*5))/5;
		}
		a {
			display: block;
			width: 100%;
			text-overflow: ellipsis;
			white-space: nowrap;
			overflow: hidden;
	
		}
	
	}
}

// ------------------------------------------------
// Showroom Pages Styles
// ------------------------------------------------
.page-content.showroom-page {
	#gradient > .vertical(@white,@lighterblue);
	border-color: transparent;
	aside.span3 {
		background-color: @lightergray;
		.border-radius(6px);
		ul.unlist li, h5 {
			padding: @gridGutterWidth/4 @gridGutterWidth;
			border-bottom: 1px solid rgba(255,255,255,.3);
			box-shadow: inset 0px -1px 0px 0px darken(@lightergray,10%);
			
		}
		ul.unlist li:last-child{
			border-bottom: none;
			box-shadow: none;	
		}
		h5.sidebar-heading {
			color: @darkestgray;
			background-color: rgba(0,0,0,.1);
			padding-top: 10px;
			margin-top: 0px;
			&.top-radius {
				.border-top-radius(4px);	
			}
		}
		li.active {
			background-color: @white;
			background-color: rgba(255,255,255,.5);	
		}
		.box-shadow(inset 0px -1px 5px rgba(0,0,0,.4));
	}
	.category {
		border-color: @darkblue;
		.caption {
			background-color: @darkblue;
			color: @white;	
		}
	}
}
.showrooms-banner, .showroom-banner {
	display:block;
	position: relative;
	.border-radius(4px);
	height: 110px;
	background: @darkblue url(../images/showrooms_banner_bg.jpg) no-repeat bottom left;
	h1 {
		font-size: 21px;
		margin: 0;
		line-height: 23px;
		font-weight: bold;
		position: absolute;
		top: 15px;
		left: 20px;	
		color: @white;
		z-index: 4;
		width: 65%;
	}
}
.showroom-banner {
	background: @darkblue url(../images/showroom_banner_bg.jpg) no-repeat bottom left;
	padding-bottom: 30px;
}
.showroom-thumbnails {
	margin-bottom: @gridGutterWidth;
	height: 170px;
	.slider-container .slider ul li {
		width: 125px;	
	}
	.slider {
		margin-top: 3px;
		height: 140px;
		img {
			display: block;
			//height: 100%;
			//max-width: none;
			width: auto;
			.border-radius(4px);
			margin: 0 auto;	
		}
		a {
			display: block;
			width: 100%;
			height: 125px;
			overflow: hidden;	
			.border-radius(4px);
			border: 3px transparent;	
		}
		a.active {
			border: 3px solid @brightblue;
		}
	}
}
.showroom-description,.showroom-main-image {
	margin-bottom: @gridGutterWidth;	
}
.showroom-main-image {
	img {
		display: block;
		margin: 0 auto;
		.border-radius(4px);
	}
}
.showroom-description {
	font-size: 18px;
	line-height: 25px;
	font-weight: lighter;
	font-weight: 200;	
}

// ------------------------------------------------
// Product Detail Page Styles
// ------------------------------------------------
.product-details {
	margin-bottom: @gridGutterWidth*2;
	.product-price {
		font-size: 18px;
		strong {
			font-size: 24px;
		}
	}
	.product-id {
	}
	.qty, .options, .options select, div.add-to-cart {
		margin-bottom: @gridGutterWidth/2;
	}
	.review-overview .pull-left { //the review stars are taller than the text...
		margin-top: -5px;
		margin-right: 10px;
	}
}
.product-image {
	border: 3px solid @white;
	.border-radius(4px);
	.box-shadow(0px 2px 5px 0px rgba(0,0,0,.2));
	display: block;
	width: 100%;
	max-height: 500px;
	overflow: hidden;
	position: relative;
	img {
		display: block;
		margin: 0 auto;
		width: 100%;
	}
}
.stock-msg {
	float: left;
	padding: 1px 3px;
	width: 50%;
	font-size: @fontSizeMini;
	color: @darkgray;
	background-color: @lightergray;
	text-align: center;
	&.out-of-stock {
		background-color: @lightyellow;	
	}
}
#attr_box {
	.attribute {
		margin-bottom: @gridGutterWidth/4;
		select {
			width: 100%;
			display: block;	
		}
	}
}
#add_to_cart.full-width {
	display: block;	
}
#show_pic.has-alternate-images {
	width: 80%;	
	float: left;
}
#thumbnails_box {
	float: right;	
}
.color-swatch-options, .product-image-thumbnails {
	.unlist;
}
.color-swatch-options li {
	float: left;
	margin-right: @gridGutterWidth/4;
	img {
		width: 20px;
		height: 20px;	
	}
}
.product-image-thumbnails li {
	width: @gridColumnWidth*.8;
	height: @gridColumnWidth*.8;
	margin-bottom: @gridGutterWidth*.8;
	a {
		display: block;
		width: 100%;
		height: @gridColumnWidth*.8;
		overflow: hidden;
		border: 3px solid @white;
		border: 3px solid @white;
		.border-radius(4px);
		.box-shadow(0px 2px 5px 0px rgba(0,0,0,.2));
		&.active, &:hover {
			border-color: @blue;	
		}
		img {
			width: 100%;
			height: auto;	
		}
	}
}
#product-detail-related-categories .category.showroom {
	width: 140px;
	margin-top: 32px;	
}

// ------------------------------------------------
