/*
// --------------------------------------------------
// --------------------------------------------------
// Template-Specific Styles
// --------------------------------------------------*/

body {
	box-shadow: inset 0px 80px 100px 0px rgba(255,255,255,.4);	
}
a {
	cursor: pointer;	
}
h1, h2 {
	font-weight: lighter;
	font-weight: 200;
	color: @darkblue;
}
h3 {
	font-weight: lighter;	
}
h1 {
	margin: 0 0 @gridGutterWidth*1.25 0;
	font-size: 30px;
	[class*=" icon"],[class^="icon"] {
		line-height: 49px;
		vertical-align: middle;	
	}
}
h1,h2,h3,h4,h5 {
	&.orange {
		color: @orange;
	}
	&.green {
		color: @green;	
	}
	&.blue {
		color: @darkblue;	
	}
}
a.orange {
	color: @orange;	
}
.btn-large [class^="icon-"], .btn-large [class*=" icon-"] {
	vertical-align: middle;
}
.hide-desktop {
	display: none;	
}

// ------------------------------------------------
// Brands
// ------------------------------------------------
.brand_head {
	padding: 5px;
	.border-radius(4px);
	.box-shadow(inset 0px 1px 3px rgba(0,0,0,.1));
	background-color: rgba(0,0,0,.1);
}
h4.brand_head {
	.clearfix;	
}
.browse-brands {
	.brand-list-alpha {
		.unlist;
		>li {
			float: left;
			a {
				.brand_head;
				display: block;
				margin-right: 4px;
				padding: 5px 6px;	
			}
		}
	}
	.brand-list {
		.unlist;
		width: 150px;
		margin-right: 15px;
		float: left;	
	}
}
// BRANDS IN NAVIGATION! 
header nav li.browse-brands {
	li.drop-heading, li.drop-table {
		width: 600px;
		.clearfix;
	}
 	ul.brand-list-alpha {
		position: static;
		min-width: 600px;
		min-height: 40px;
		.box-shadow(none);
		.border-radius(0);
		margin-left: 10px;
		> li a{
			margin-top: 10px;
			font-size: 13px;
			color: @darkblue;	
			font-weight: bold;
			margin-right: 2px;
			padding: 5px;	
			&:hover {
				background-color: @white;	
			}
			:first-child {
				margin-left: 10px;	
			}
		}
	}
	ul.brand-list {
		a {
			display: block;
			padding: 3px;
			.border-radius(4px);
			margin: 5px;	
		}
	}
}

// ------------------------------------------------
// Layout
// ------------------------------------------------
header.container {
	box-shadow:0px 0px 25px rgba(0,0,0,.25), 0px 60px 80px 15px rgba(255,255,255,.4),0px 0px 150px 35px rgba(255,255,255,.7);
	min-height: 155px;
	border: none 0;
	padding: 0px @gridGutterWidth 2px;
	position: relative;
	background-color: #fff;
