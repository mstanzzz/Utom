#
Options +FollowSymlinks
#RewriteBase /
RewriteEngine on
#
# prevent infinite loops
RewriteCond %{ENV:REDIRECT_STATUS} 200
RewriteRule .* - [L]
#
#
RewriteRule ^$ template_default.php?slug=home [L]
#
RewriteRule ^product-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$1 [L]
RewriteRule ^([^.]+)/product-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$2 [L]
#
RewriteRule ^showroom-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$1 [L]
RewriteRule ([^.]+)/showroom-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$2 [L]
RewriteRule ([^.]+)/([^.]+)/showroom-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$3 [L]
RewriteRule ([^.]+)/([^.]+)/([^.]+)/showroom-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$4 [L]
RewriteRule ([^.]+)/([^.]+)/([^.]+)/([^.]+)/showroom-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$5 [L]
#
RewriteRule ^/.html$ template_default.php?slug=home [L]
RewriteRule ^/$ template_default.php?slug=home [L]
RewriteRule ^/([^/\.]+)/?$ template_default.php?slug=home [L]
#
RewriteRule ^([^/\.]+)/function.mysql-query template_default.php?slug=home [L]
RewriteRule ^([^/\.]+)/([^/\.]+)/function.mysql-query template_default.php?slug=home [L]
RewriteRule ^([^/\.]+)/([^/\.]+)/([^/\.]+)/function.mysql-query template_default.php?slug=home [L]
#
RewriteRule ^([^/\.]+)/.html?$  template_default.php?slug=home [L]
#
RewriteRule	^closet-accessory-cart.html shopping-cart.html
#
#RewriteRule ^idea-folder.html?$ ideastemplate.php?slug=idea-folder
#RewriteRule ^([^/\.]+)/idea-folder.html?$ ideastemplate.php?slug=idea-folder
#
# store product
#site.com/{global-word}/{cat1}/{cat2}/{item-name}/product.html?id=
RewriteRule ^product.html$ template_shop_details.php [L]
RewriteRule ^([^.]+)/product.html$ template_shop_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/product.html$ template_shop_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/([^.]+)/product.html$ template_shop_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/([^.]+)/([^.]+)/product.html$ template_shop_details.php [L]
#
# showroom product
RewriteRule ^showroom-product\.html$ template_showroom_details.php [L]
RewriteRule ^([^.]+)/showroom-product.html$ template_showroom_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/showroom-product.html$ template_showroom_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/([^.]+)/showroom-product.html$ template_showroom_details.php [L]
RewriteRule ^([^.]+)/([^.]+)/([^.]+)/([^.]+)/showroom-product.html$ template_showroom_details.php [L]
#
#store category page after 08/13/2015
#/category-64/decorative-hardware-handles-knobs-hooks.html
RewriteRule ^category-([0-9]+)/([^/\.]+).html?$ template_shop.php?prodCatId=$1 [L]
RewriteRule ^([^.]+)/category-([0-9]+)/([^/\.]+).html?$ template_shop.php?prodCatId=$2 [L]
RewriteRule ^([^.]+)/([^.]+)/category-([0-9]+)/([^/\.]+).html?$ template_shop.php?prodCatId=$3 [L]
#
#
# showroom category page after 08/13/2015
#/showroom-category-19/custom-garage-organizers.html
RewriteRule ^showroom-category-([0-9]+)/([^/\.]+).html?$ template_default.php?slug=showroom&prodCatId=$1 [L]
RewriteRule ^([^.]+)/showroom-category-([0-9]+)/([^/\.]+).html?$ template_default.php??slug=showroom&prodCatId=$2 [L]
RewriteRule ^([^.]+)/([^.]+)/showroom-category-([0-9]+)/([^/\.]+).html?$ template_default.php??slug=showroom&prodCatId=$3 [L]
#
#shopping cart
RewriteRule ^shopping-cart.html?$ shopping-cart.php [L]
RewriteRule ^([^/\.]+)/shopping-cart.html?$ shopping-cart.php [L]

#checkout
RewriteRule ^checkout.html?$ checkout.php [L]
RewriteRule ^([^/\.]+)/checkout.html?$ checkout.php [L]
#Confirmation
RewriteRule ^process-order.html?$ process-order.php [L]
RewriteRule ^([^/\.]+)/process-order.html?$ process-order.php [L]
#
RewriteRule ^order.html?$ order.php [L]
RewriteRule ^([^/\.]+)/order.html?$ order.php [L]
#
#blog
#RewriteRule ^blog.html$ template_default.php
#RewriteRule ^([^/\.]+)/blog.html$ template_default.php
#RewriteRule ^([^/\.]+)/([^/\.]+)/blog.html$ template_default.php
#RewriteRule ^([^/\.]+)/([^/\.]+)/([^/\.]+).blog.html$ template_default.php
#
#fixed pages
#site.com/{global-word}/{seo-page-name}.html?id=
RewriteRule ^([^/\.]+).html?$ template_default.php?slug=$1 [QSA,L]
#RewriteRule ^([^/\.app]+)/([^/\.]+).html?$ template_default.php?slug=$2 [QSA,L]
RewriteRule ^(?!app)([^/\.]+)/([^/\.]+).html?$ template_default.php?slug=$2 [QSA,L]
#
RewriteRule ^([^/\.]+)/catalog/([0-9]+)/?$ template_default.php?seo=$1&slug=showroom-sub-category&sr_cat_id=$2 [L]
RewriteRule ^([^/\.]+)/catalog/([^/\.]+)/([0-9]+)/?$ template_default.php?seo=$1&slug=showroom-sub-category&cat_name=$2&sr_cat_id=$3 [L]
RewriteRule ^([^/\.]+)/catalog.html?$ template_default.php?seo=$1&slug=showroom-main [L]
RewriteRule ^([^/\.]+)/blog-cat/([0-9]+)/?$ template_default.php?seo=$1&slug=$1&blog-cat=$2 [L]
RewriteRule ^([^/\.]+)/news.html$ template_default.php?seo=$1&type=news [L]
RewriteRule ^([^/\.]+)/blog-page/([0-9]+)/?$ template_default.php?seo=$1&slug=$1&blog-page=$2 [L]
RewriteRule ^([^/\.]+)/blog-post/([0-9]+)/?$ template_default.php?seo=$1&slug=$1&blog-post=$2 [L]
#
Options -Indexes
#
ErrorDocument 404 /404.html
#
#dynamic content
#::::::

